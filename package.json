{
  "name": "gateio-p2p-scraper",
  "version": "1.1.0",
  "description": "Scrape Gate.io P2P listings to JSON/CSV with robust logging and config.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "NODE_ENV=development DEBUG=true node src/index.js",
    "start:prod": "NODE_ENV=production node src/index.js",
    "start:staging": "NODE_ENV=staging node src/index.js",
    "scrape:once": "node -e \"require('./scraper/enhanced-scraper').runScraper()\"",
    "healthcheck": "node scripts/healthcheck.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "jest --passWithNoTests",
    "legacy:start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salaamdev/gateio-p2p-scraper.git"
  },
  "keywords": [
    "gateio",
    "p2p",
    "scraper",
    "puppeteer"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/salaamdev/gateio-p2p-scraper/issues"
  },
  "homepage": "https://github.com/salaamdev/gateio-p2p-scraper#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "joi": "^18.0.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "puppeteer": "^24.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3"
  }
  ,
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  }
}
